<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <VBox>
        <SplitPane dividerPositions="0.3" VBox.vgrow="ALWAYS">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                <ListView fx:id="newsListView" layoutX="-4.0" layoutY="53.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            </AnchorPane>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                <VBox layoutX="150.0" layoutY="55.0" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <Label fx:id="title" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="38.0" prefWidth="430.0" wrapText="true">
                        <font>
                            <Font name="System Bold" size="24.0" />
                        </font>
                        <padding>
                            <Insets left="10.0" right="10.0" top="10.0" />
                        </padding>
                    </Label>
                    <Label fx:id="publicationDate" prefHeight="5.0" prefWidth="417.0">
                        <padding>
                            <Insets left="10.0" right="10.0" />
                        </padding>
                    </Label>
                    <TextArea fx:id="description" editable="false" wrapText="true" VBox.vgrow="ALWAYS">
                        <VBox.margin>
                            <Insets top="10.0" />
                        </VBox.margin>
                    </TextArea>
                    <ButtonBar prefHeight="40.0" prefWidth="200.0">
                        <buttons>
                            <Button fx:id="previousButton" mnemonicParsing="false" text="&lt;-" />
                            <Button fx:id="nextButton" mnemonicParsing="false" text="-&gt;" />
                        </buttons>
                    </ButtonBar>
                </VBox>
            </AnchorPane>
        </SplitPane>
    </VBox>
</StackPane>
